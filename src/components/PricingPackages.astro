</section>

<style>
  .pricing-card {
    background: white;
    border-radius: 22px;
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    position: relative;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  }

  .pricing-card:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 50px rgba(0, 0, 0, 0.15);
  }

  .pricing-card.featured {
    border: 2px solid #3197ff;
    box-shadow: 0 10px 40px rgba(49, 151, 255, 0.2);
  }

  .pricing-card.featured:hover {
    box-shadow: 0 20px 60px rgba(49, 151, 255, 0.3);
  }

  .badge {
    position: absolute;
    top: 20px;
    right: 20px;
    background: #3197ff;
    color: white;
    padding: 6px 16px;
    border-radius: 20px;
    font-size: 14px;
    font-weight: 600;
    z-index: 10;
  }

  .card-image-wrapper {
    width: 100%;
    height: 260px;
    overflow: hidden;
    position: relative;
    background: #f3f4f6;
  }

  .parallax-image {
    width: 100%;
    height: 100%;
    object-fit: contain;
    transition: transform 0.3s ease;
  }

  .parallax-card:hover .parallax-image {
    transform: scale(1.05);
  }

  .card-content {
    padding: 30px 24px;
  }

  .card-title {
    color: #1a1a1a;
    font-size: 24px;
    font-weight: 700;
    margin-bottom: 12px;
  }

  .card-subtitle {
    color: #3197ff;
    font-size: 15px;
    font-weight: 600;
    margin-bottom: 20px;
    line-height: 1.4;
  }

  .card-benefits {
    color: #6b7280;
    font-size: 12px;
    text-align: center;
    margin-top: 12px;
    line-height: 1.5;
  }

  .card-features {
    list-style: none;
    padding: 0;
    margin: 0 0 24px 0;
  }

  .card-features li {
    color: #4b5563;
    font-size: 16px;
    margin-bottom: 12px;
    display: flex;
    align-items: center;
    gap: 10px;
  }

  .check-icon {
    width: 20px;
    height: 20px;
    color: #3197ff;
    flex-shrink: 0;
  }

  .card-button {
    display: block;
    width: 100%;
    padding: 14px;
    background: white;
    border: 2px solid #3197ff;
    color: #3197ff;
    text-align: center;
    border-radius: 12px;
    font-weight: 600;
    font-size: 16px;
    text-decoration: none;
    transition: all 0.3s ease;
  }

  .card-button:hover {
    background: #3197ff;
    color: white;
    transform: scale(1.02);
  }

  .featured-button {
    background: #3197ff;
    color: white;
  }

  .featured-button:hover {
    background: #2880e0;
    transform: scale(1.02);
  }

  .card-button-secondary {
    display: block;
    width: 100%;
    padding: 12px;
    background: white;
    border: 2px solid #E07B47;
    color: #E07B47;
    text-align: center;
    border-radius: 12px;
    font-weight: 600;
    font-size: 15px;
    text-decoration: none;
    transition: all 0.3s ease;
    margin-top: 10px;
  }

  .card-button-secondary:hover {
    border-color: #d16a35;
    color: #d16a35;
    background: #fef3ec;
    transform: scale(1.01);
  }

  /* Mobile responsive */
  @media (max-width: 768px) {
    .card-title {
      font-size: 20px;
    }

    .card-subtitle {
      font-size: 14px;
    }

    .card-content {
      padding: 24px 20px;
    }

    .card-image-wrapper {
      height: 200px;
    }
  }
</style>

